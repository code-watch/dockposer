{
    "name": "ntavelis/dockposer",
    "description": "Composer plugin that automatically generates docker files.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Athanasios Ntavelis",
            "email": "davelis89@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "composer-plugin-api": "^1.1",
        "league/flysystem": "^1.0.67"
    },
    "require-dev": {
        "composer/composer": "^1.1.3",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Ntavelis\\Dockposer\\": "src/"
        }
    },
    "extra": {
        "class": "Ntavelis\\Dockposer\\DockposerPlugin"
    }
}
